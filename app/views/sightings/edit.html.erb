<h1>Edit sighting:</h1>

<%= form_for [@animal, @sighting] do |f| %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :longitude %>
  <%= f.number_field :longitude %>
  <%= f.label :latitude %>
  <%= f.number_field :latitude %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <p><%= f.submit %></p>
<% end %>
